<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>RockDb.Native</id>
    <version>6.11.0</version>
    <authors>Skird</authors>
    <owners>Skird</owners>
    <licenseUrl>https://github.com/facebook/rocksdb/blob/master/COPYING</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/Skird/rocksdb</projectUrl>
    <description>This package contains platform-specific native code builds of RockDB (with no libsnappy dependency). Included: linux-x64/librocksdb.so, osx-x64/librocksdb.dylib, win-x64/rockdb.dll</description>
    <tags>rocksdb embedded database</tags>
  </metadata>
  <files>
    <file src="lib/linux-x64/librocksdb.so" target="runtimes/linux-x64/native/librocksdb.so" />
    <file src="lib/osx-x64/librocksdb.dylib" target="runtimes/osx-x64/native/librocksdb.dylib" />
    <file src="lib/win-x64/rocksdb.dll" target="runtimes/win-x64/native/rocksdb.dll" />
    <file src="RocksDb.Native.targets" target="build/netstandard1.0"/>
  </files>
</package>
